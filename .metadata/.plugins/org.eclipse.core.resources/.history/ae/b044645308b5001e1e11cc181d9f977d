package hello_java_world;

public class ArithmaticOperator {
	/**
	 * 실행을 위한 코드
	 * public static void main(String[] args)
	 * @param args
	 */
	public static void main(String[] args) {
		int number1 = 10;
		number1 = number1 + 2;
		System.out.println(number1);
		
		int number2 = 10;
		number2 = number2 - 10;
		System.out.println(number2);
		
		int number3 = 10;
		number3 = number3 *3;
		
		int number4 = 10;
		number4 = number4 / 4;
		System.out.println(number4);
	}
}
