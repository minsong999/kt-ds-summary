package oop_exam;

public class MethodExam {

	public static void methodExam1() {
		int result = 0;
		for (int i = 1; i < 101; i++) {
			result += i;
		}
		System.out.println(result);
	}

	public static void methodExam2() {
		int result2 = 0;
		for (int i = 1; i < 101; i++) {
			if (i % 2 == 1) {
				result2 += i;
			}
		}
		System.out.println(result2);
	}

	public static void methodExam3() {
		for (int i = 1; i < 101; i++) {
			if (i % 3 == 0 && i % 5 == 0 && i % 6 == 0) {
				System.out.println(i);
			}
		}
	}
	
	public static void methodExam4() {
		for (int i = 1; i < 6; i++) {
			for (int j = 1; i >= j; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
	
	public static void test(int ... number) {
		System.out.println(number);
	}
	
	public static void main(String[] args) {
		methodExam1();
		methodExam2();
		methodExam3();
		methodExam4();
		
		test(1,3,4);
	}
}
